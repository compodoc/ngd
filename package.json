{
  "name": "angular2-dependencies-graph",
  "version": "1.0.0-alpha.6",
  "description": "A tool to generate a component dependencies graph of an Angular2 application (written in typescript)",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "tsc -p test/tsconfig-test.json",
    "test": "mocha --opts test/mocha.opts",
    "prepublish": "npm run tsc",
    "deploy": "npm run tsc && npm version patch && npm publish",
    "tsc": "tsc --newLine LF"
  },
  "bin": {
    "ngd": "bin/index-cli.js"
  },
  "homepage": "https://github.com/manekinekko/angular2-dependencies-graph",
  "bugs": "https://github.com/manekinekko/angular2-dependencies-graph/issues",
  "author": "Wassim CHEGHAM",
  "engines": {
    "node": ">= 5.4.1 < 7"
  },
  "contributors": [
    "vogloblinsky"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manekinekko/angular2-dependencies-graph.git"
  },
  "private": false,
  "license": "MIT",
  "dependencies": {
    "colors": "1.1.2",
    "commander": "2.9.0",
    "dot": "1.0.3",
    "fs-extra": "0.26.2",
    "gulp-util": "3.0.7",
    "opener": "1.4.1",
    "q": "1.4.1",
    "svg-to-png": "2.0.1",
    "typescript": "2.0.0",
    "viz.js": "1.3.0"
  },
  "devDependencies": {
    "@types/dot": "1.0.29",
    "@types/typescript": "0.4.29",
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.32",
    "@types/node": "6.0.39",
    "chai": "3.5.0",
    "mocha": "3.1.2"
  }
}
