digraph dependencies {
  node[shape="{2}",style="filled",colorscheme={scheme}]

  {{~it.modules :module}}
    "{{= module.name }}"[label="{{= module.name }}", color=1]
  {{~}}

  node[shape="{2}",style="filled",color=2]
  {{~it.modules :module}}
    {{~module.modules :dependency}}
      "{{= dependency }}" -> "{{= module.name }}"
    {{~}}
  {{~}}
}